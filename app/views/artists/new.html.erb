<h1>New Artist</h1>

<% if flash[:errors] %>
  <% flash[:errors].each do |error| %>
    <p><%= error %></p>
  <% end %>
<% end %>

<%= form_for @artist do |f| %>
   <%= f.label :name  %>
   <%= f.text_field :name  %>
   
   <%= f.label :title  %>
   <%= f.text_field :title  %>

   <%= f.label :age  %>
   <%= f.text_field :age  %>

   <%= f.submit %>
<% end %>